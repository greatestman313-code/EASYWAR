{
  "name": "chat-easy-war",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint || echo 'no lint'",
    "test:unit": "cross-env TEST_ENABLE_MOCK_AUTH=1 NODE_ENV=test vitest run",
    "test:e2e": "cross-env TEST_ENABLE_MOCK_AUTH=1 NODE_ENV=test playwright test",
    "test:e2e:ui": "cross-env TEST_ENABLE_MOCK_AUTH=1 NODE_ENV=test playwright test --ui",
    "test": "npm run test:unit && npm run test:e2e"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "@supabase/ssr": "^0.5.0",
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.46.0",
    "@types/node": "^20.11.30",
    "autoprefixer": "^10.4.18",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
